<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">
    <!-- INTEGRATION START -->
    <bean class="com.cgi.hybris.payment.core.data.PaymentModeData">
        <description>The Payment Mode (creditcard, invoice, advance, etc.)</description>
        <property name="orderPriority" type="java.lang.Integer">
            <description>The display name</description>
        </property>
    </bean>
    <!-- INTEGRATION END -->

    <bean class="de.hybris.platform.commercefacades.product.data.ProductData">
        <property name="productType" type="String"/>
    </bean>

    <bean class="de.hybris.platform.commercefacades.order.data.AbstractOrderData">
        <property name="redirectUrl" type="String"/>
    </bean>

    <bean class="com.cgi.pacoshop.facades.user.data.Title2Data">
        <property name="code" type="String"/>
        <property name="name" type="String"/>
    </bean>
</beans>
